  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Focus",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": ["--flavor", "production", "--target", "lib/main.dart"]
    },
    {
      "name": "Launch Focus Profile",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": ["--flavor", "production", "--target", "lib/main.dart", "--profile"]
    },
    {
      "name": "Launch Storybook",
      "request": "launch",
      "type": "dart",
      "program": "lib/storybook/storybook_main.dart",
      "args": ["--flavor", "production", "--target", "lib/storybook/storybook_main.dart", "--profile"]
    }
  ]
}
